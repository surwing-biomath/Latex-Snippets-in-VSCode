        "body": [
            "\\begin{tikzpicture}",
            "    \\begin{axis}[",
            "        legend pos=outer north east,",
            "        title={${1:Plot Title}},",
            "        axis lines=${2|center,left,right,box|},",
            "        xlabel={${3:x-axis label}},",
            "        ylabel={${4:y-axis label}},",
            "    ]",
            "        $0",
            "    \\end{axis}",
            "\\end{tikzpicture}"
        ],
        "description": "Create a 2D plotting environment."
    },

    "Plot 2D Graph": {
        "prefix": ["plotgraph2d", "\\plotgraph2d"],
        "body": [
            "\\addplot [",
            "    domain=${1:-10}:${2:10},",
            "    samples=${3:100},",
            "    color=${4:blue},",
            "]",
            "    {${5:expression}};",
            "\\addlegendentry{${6:Legend Entry}}",
            "$0"
        ],
        "description": "Plot a 2D graph within a PGFPlots environment."
