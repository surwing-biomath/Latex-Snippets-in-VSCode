"begin": {
	"prefix": "beg",
	"body": [
		"\\begin{${1|definition,thm,nthm,lemma,nlemma,prop,nprop,cor,ncor,claim,proof,ppt,rmk,eg,axiom,notation,assumption,conjecture,fact,question,ex,solution|}}","\t$2","\\end{$1} $0",
	],
	"description": "begin"
},
"itemize": {
	"prefix": "bit",
	"body": [
		"\\begin{itemize}",
		"    \\item $1 \frac{}{} ",
		"    \\item   ",
		"    \\end{itemize} "
	],
	"description": "itemize"
},
